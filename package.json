    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@web/dev-server": "0.1.30",
    "@web/dev-server-esbuild": "0.2.16",
    "@web/test-runner": "0.13.27",
    "@web/test-runner-puppeteer": "^0.10.5",
    "esbuild": "^0.11.23",
    "eslint": "^7.32.0",
