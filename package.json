{
  "name": "graphlib",
  "version": "0.1.0",
  "description": "This is a full featured graph library with a multitude of capabilities going beyond the core library of cytoscape.",
  "author": "Jillian Ada Burrows <jill@adaburrows.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adaburrows/graphlib/issues"
  },
  "homepage": "https://github.com/adaburrows/graphlib#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "lint": "npx eslint . --ignore-path .gitignore --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest",
    "test:ci": "npm test",
    "test:watch": "npm test -- --watch --verbose",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adaburrows/graphlib.git"
  },
  "keywords": [
    "graph",
    "topology",
    "graph traversal",
    "matrix",
    "spectral graph theory"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
